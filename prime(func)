#include<stdio.h>
void prime(int);
int main()
{
int n;
printf("enter n");
scanf("%d",&n);
prime(n);
}
void prime(int n)
{
int i=1,c=0;
while(i<=n)
{
if(n%i==0)
c=c+1;
i=i+1;
}
if(c==2)
printf('given number is prime");
else
printf("given number is not prime");
}
OUTPUT:
enter n2
given number is prime
